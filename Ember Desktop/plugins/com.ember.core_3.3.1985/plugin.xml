  <!-- Editors -->
  <extension point="org.eclipse.ui.editors">
    <editor
          class="com.ember.workbench.gui.editor.StreamEditor"
          contributorClass="com.ember.workbench.gui.editor.StreamEditorContributor"
          icon="icons/ember.png"
          id="com.ember.workbench.gui.editor.StreamEditor"
          name="Stream">
    </editor>
    <editor
          class="com.ember.workbench.gui.editor.LargeFileEditor"
          contributorClass="com.ember.workbench.gui.editor.LargeFileEditorContributor"
          icon="icons/ember_large.png"
          id="com.ember.workbench.gui.editor.LargeFileEditor"
          name="Large File">
    </editor>
  </extension>
  
  <!-- Element factories -->
  <extension point="org.eclipse.ui.elementFactories">
    <factory 
      id="com.ember.workbench.gui.editor.StreamEditorFactory"
      class="com.ember.workbench.gui.editor.StreamEditorFactory">
    </factory>
  </extension>
  
  <!-- Views -->
  <extension point="org.eclipse.ui.views">
    <category id="com.ember.workbench.gui.view" name="%view.category.name"/>
      <view
       name="%view.progress.name"
       icon="icons/progress.gif"
       category="com.ember.workbench.gui.view"
       class="org.eclipse.ui.ExtensionFactory:progressView"
       id="org.eclipse.ui.views.ProgressView"/>
   <view id="com.ember.workbench.gui.view.AdapterView" 
      name="%view.backchannel.name" 
      class="com.ember.workbench.gui.view.AdapterView" 
      category="com.ember.workbench.gui.view"
      icon="icons/backchannel.gif"
      allowMultiple="true" />    
    <view id="com.ember.workbench.gui.view.PacketView" 
      name="%view.packet.name" 
      class="com.ember.workbench.gui.view.PacketView" 
      category="com.ember.workbench.gui.view"
      icon="icons/packetview.gif"
      allowMultiple="true" />
    <view id="com.ember.workbench.gui.view.BytesView" 
      name="%view.bytes.name" 
      class="com.ember.workbench.gui.view.BytesView" 
      category="com.ember.workbench.gui.view"
      icon="icons/binary.png"
