     <page
           category="com.ember.workbench.gui.preference.PreferencePage.file"
           class="com.ember.workbench.gui.preference.NetworkPreferencePage"
           id="com.ember.workbench.gui.preference.NetworkPreferencePage"
           name="Decoding">
     </page>
     <page
           category="com.ember.workbench.gui.preference.PreferencePage.file"
           class="com.ember.workbench.gui.preference.NodeIconPreferencePage"
           id="com.ember.workbench.gui.preference.NodeIconPreferencePage"
           name="Node Icons">
     </page>
     <page
           category="com.ember.workbench.gui.preference.PreferencePage.file"
           class="com.ember.workbench.gui.preference.PreferencePage"
           id="com.ember.workbench.gui.preference.PreferencePage.stream"
           name="Stream">
     </page>
     <page
           category="com.ember.workbench.gui.preference.PreferencePage.file"
           class="com.ember.workbench.gui.preference.PreferencePage"
           id="com.ember.workbench.gui.preference.PreferencePage.timeline"
           name="Timeline">
     </page>
     <page name="Network Analyzer"
