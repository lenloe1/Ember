           name="Wireless Adapter Options">
     </page>
     <page name="InSight Adapters"
        category="com.silabs.foundation.device.ui.preference.DeviceManagerPreferencePage"
     	class="com.ember.workbench.gui.preference.UDPDiscoveryPreferencePage"
     	id="com.ember.workbench.gui.preference.UDPDiscoveryPreferencePage">
     </page>
     <!--<page name="TCP"
        category="com.ember.workbench.gui.preference.BackchannelPreferencePage"
        class="com.ember.workbench.gui.preference.TCPDiscoveryPreferencePage"
     	id="com.ember.workbench.gui.preference.TCPDiscoveryPreferencePage">
     </page> -->
     <page
           category="com.ember.workbench.gui.preference.BackchannelPreferencePage"
           class="com.ember.workbench.gui.preference.AppUploadPreferencePage"
           id="com.ember.workbench.gui.preference.AppUploadPreferencePage"
           name="Loaders and Binary Images">
     </page>
     <page
           category="com.ember.workbench.gui.preference.NetworkPreferencePage"
           class="com.ember.workbench.gui.preference.SecurityPreferencePage"
           id="com.ember.workbench.gui.preference.SecurityPreferencePage"
           name="Security Keys">
     </page>
     <page
           category="com.ember.workbench.gui.preference.NetworkPreferencePage"
           class="com.ember.workbench.gui.preference.StackPreferencePage"
           id="com.ember.workbench.gui.preference.StackPreferencePage"
           name="Stack Versions">
     </page>
     <page
           category="com.ember.workbench.gui.preference.NetworkPreferencePage"
           class="com.ember.workbench.gui.preference.GrouperPreferencePage"
           id="com.ember.workbench.gui.preference.GrouperPreferencePage"
           name="Transaction groupers">
     </page>
     <page
           category="com.ember.workbench.gui.preference.PreferencePage.file"
           class="com.ember.workbench.gui.preference.ConnectivityPreferencePage"
           id="com.ember.workbench.gui.preference.ConnectivityPreferencePage"
           name="Connectivity Display">
     </page>
     <page
           category="com.ember.workbench.gui.preference.NetworkPreferencePage"
           class="com.ember.workbench.report.ReportingPreferencePage"
           id="com.ember.workbench.report.ReportingPreferencePage"
           name="Reports">
     </page>
     <page
