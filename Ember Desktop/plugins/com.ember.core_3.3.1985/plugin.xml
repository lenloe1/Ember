    <objectContribution
          adaptable="false"
          id="com.ember.workbench.adapter.IAdapter.object"
          objectClass="com.ember.workbench.adapter.IAdapterProvider">
       <action
             class="com.ember.workbench.gui.action.AdapterPropertiesAction"
             enablesFor="1"
             id="com.ember.workbench.gui.action.AdapterPropertiesAction"
             label="Adapter properties..."
             menubarPath="additions"
             style="push">
