  </extension>
    
  <!-- Perspective Extension Point -->
  <extension
        id="perspective"
        name="Main"
        point="org.eclipse.ui.perspectives">
    <perspective
          class="com.ember.workbench.gui.DefaultPerspective"
          fixed="false"
          icon="icons/perspective_na.png"
          id="com.ember.workbench.gui.DefaultPerspective"
          name="Network Analyzer">
       <description>
          Network analyzer perspective allows user to focus on capturing and analyzing network traffic.
       </description>
    </perspective>
  </extension>
  
  
  <!-- Pop up menu extensions -->
  <extension point="org.eclipse.ui.popupMenus">
      
    <!-- Organize Map -->
      
    <!-- Packet entry: popups for the packet view tree nodes -->
    <objectContribution
          adaptable="false"
          objectClass="com.ember.workbench.gui.view.IPacketEntry"
          id="com.ember.workbench.gui.view.IPacketEntry.Contribution">
      <action
        id="com.ember.workbench.gui.action.PacketViewAction.INFO"
        label="Information..."
        style="push"
        menubarPath="additions"
        enablesFor="1"
        class="com.ember.workbench.gui.action.PacketViewAction">
      </action>
    </objectContribution>
    
    <objectContribution
          adaptable="false"
          objectClass="com.ember.workbench.gui.view.IPacketFrame"
          id="com.ember.workbench.gui.view.IPacketFrame.Contribution">
      <action
        id="com.ember.workbench.gui.action.FrameMatchAction"
        label="Filter by frame pattern..."
        style="push"
        menubarPath="additions"
        enablesFor="1"
        class="com.ember.workbench.gui.action.FrameMatchAction">
      </action>
    </objectContribution>
    
    <!-- Packet entry: popups for the packet field tree nodes -->
    <objectContribution
      adaptable="false"
      objectClass="com.ember.workbench.gui.view.IPacketField"
      id="com.ember.workbench.gui.view.IPacketField.Contribution">
      <action
        id="com.ember.workbench.gui.action.PacketViewAction.HEX_DEC_TOGGLE"
        label="Hex/decimal toggle"
        style="push"
        menubarPath="additions"
        enablesFor="1"
        class="com.ember.workbench.gui.action.PacketViewAction">
      </action>
      <action
        id="com.ember.workbench.gui.action.PacketViewAction.FILTER_APPLY"
        label="Replace filter"
        style="push"
        menubarPath="additions"
        enablesFor="1"
        class="com.ember.workbench.gui.action.PacketViewAction">
      </action>
      <action
        id="com.ember.workbench.gui.action.PacketViewAction.FILTER_INSERT"
        label="Add to filter"
        style="push"
        menubarPath="additions"
        enablesFor="1"
        class="com.ember.workbench.gui.action.PacketViewAction">
      </action>
    </objectContribution>
    
    <objectContribution
      adaptable="true"
      objectClass="com.ember.workbench.capture.event.BookmarkEvent"
      id="com.ember.workbench.core.UserEvent.Contribution">
      <action
        id="com.ember.workbench.gui.action.RemoveEvent"
        label="Remove"
        style="push"
        menubarPath="additions"
        enablesFor="1"
        class="com.ember.workbench.gui.action.RemoveEvent">
      </action>
      <action
        id="com.ember.workbench.gui.action.RenameEvent"
        label="Rename..."
        style="push"
        menubarPath="additions"
        enablesFor="1"
        class="com.ember.workbench.gui.action.RenameEvent">
      </action>
    </objectContribution>
    <!-- GroupEvent -->
    <objectContribution
      adaptable="false"
      objectClass="com.ember.workbench.group.GroupEvent"
      id="com.ember.workbench.GroupEvent.Contribution">
      <action
        id="com.ember.workbench.gui.action.EventAction.FILTER_BY_PACKETCOUNT"
        label="Show only Packet Count"
        style="push"
        menubarPath="filters"
        enablesFor="1"
        class="com.ember.workbench.gui.action.EventAction">
      </action>
      <action
        id="com.ember.workbench.gui.action.EventAction.FILTER_BY_MACRETRIES"
        label="Show only Mac Retries"
        style="push"
        menubarPath="filters"
        enablesFor="1"
        class="com.ember.workbench.gui.action.EventAction">
      </action>
      <action
        id="com.ember.workbench.gui.action.EventAction.FILTER_BY_ENDTOENDRETRIES"
        label="Show only End to End Retries"
        style="push"
        menubarPath="filters"
        enablesFor="1"
        class="com.ember.workbench.gui.action.EventAction">
      </action>
    </objectContribution>
    <!-- IEvent -->
    <objectContribution
      adaptable="false"
      objectClass="com.ember.workbench.capture.event.IEvent"
      id="com.ember.workbench.core.IEvent.Contribution">
      <action
        id="com.ember.workbench.gui.action.EventAction.HIDE_TYPE"
        label="Hide type"
        style="push"
        menubarPath="filters"
        enablesFor="1"
        class="com.ember.workbench.gui.action.EventAction">
      </action>
      <action
        id="com.ember.workbench.gui.action.EventAction.SHOW_ONLY_TYPE"
        label="Show only type"
        style="push"
        menubarPath="filters"
        enablesFor="1"
        class="com.ember.workbench.gui.action.EventAction">
      </action>
      <action
        id="com.ember.workbench.gui.action.ExtractEventToLog.EXTRACT"
        label="Extract to..."
        style="push"
        menubarPath="extractions"
        enablesFor="+"
        class="com.ember.workbench.gui.action.ExtractEventToLog">
      </action>
      <action
        id="com.ember.workbench.gui.action.ExtractEventToLog.APPEND"
        label="Append to file"
        style="push"
        menubarPath="extractions"
        enablesFor="+"
        class="com.ember.workbench.gui.action.ExtractEventToLog">
      </action>
      <action
        id="com.ember.workbench.gui.action.Bookmark"
        label="Add bookmark..."
        style="push"
        menubarPath="additions"
        enablesFor="1"
        class="com.ember.workbench.gui.action.Bookmark">
      </action>
      <action
        id="com.ember.workbench.gui.action.EventAction.FILTER_BY_STATUS"
        label="Show only Status"
        style="push"
        menubarPath="filters"
        enablesFor="1"
        class="com.ember.workbench.gui.action.EventAction">
      </action>
      <action
        id="com.ember.workbench.gui.action.EventAction.FILTER_BY_SOURCE"
        label="Show only source"
        style="push"
        menubarPath="filters"
        enablesFor="1"
        class="com.ember.workbench.gui.action.EventAction">
      </action>
      <action
        id="com.ember.workbench.gui.action.EventAction.FILTER_BY_DESTINATION"
        label="Show only destination"
        style="push"
        menubarPath="filters"
        enablesFor="1"
        class="com.ember.workbench.gui.action.EventAction">
      </action>
      <action
        id="com.ember.workbench.gui.action.EventAction.FILTER_BY_SUMMARY"
        label="Show only summary"
        style="push"
        menubarPath="filters"
        enablesFor="1"
        class="com.ember.workbench.gui.action.EventAction">
      </action>
      <action
            class="com.ember.workbench.gui.action.AnchorTime"
            enablesFor="1"
            id="com.ember.workbench.gui.action.AnchorTime.ANCHOR"
            label="Set zero-time anchor to this event"
            menubarPath="anchor"
            style="push"/>
      <action
            class="com.ember.workbench.gui.action.AnchorTime"
            enablesFor="+"
            id="com.ember.workbench.gui.action.AnchorTime.UNANCHOR"
            label="Unset zero-time"
            menubarPath="anchor"
            style="push"/>
    </objectContribution>
    
    <!-- IFilters -->
    <objectContribution
      adaptable="true"
      objectClass="com.ember.workbench.filter.FilterModelEntry"
      id="com.ember.workbench.filter.FilterModelEntry.Contribution">
    </objectContribution>
        
    <!-- Drawn node entries: popups for the node on the map -->
    <objectContribution
          adaptable="true"
          objectClass="com.ember.workbench.gui.view.IDrawnNode"
          id="com.ember.workbench.gui.view.IDrawnNode.Contribution">
      <action
            class="com.ember.workbench.gui.action.NodeAction"
            enablesFor="1"
            id="com.ember.workbench.gui.action.NodeAction.APPLICATION"
            label="Application..."
            menubarPath="object_actions"
            state="false"
            style="push">
      </action>
      <action
            class="com.ember.workbench.gui.action.NodeAction"
            enablesFor="1"
            id="com.ember.workbench.gui.action.NodeAction.NODECONTROL"
            label="Show node control"
            menubarPath="object_actions"
            state="false"
            style="toggle">
      </action>
      <action
            class="com.ember.workbench.gui.action.NodeAction"
            enablesFor="1"
            id="com.ember.workbench.gui.action.NodeAction.CONNECTIVITY"
            label="Show connectivity"
            menubarPath="object_actions"
            state="false"
            style="toggle">
      </action>
      <action
        id="com.ember.workbench.gui.action.NodeAction.SHOW_ONLY_PAN"
        label="Show only this PAN"
        style="push"
        menubarPath="object_actions"
        enablesFor="1"
        class="com.ember.workbench.gui.action.NodeAction">
      </action>
      <action
        id="com.ember.workbench.gui.view.IDrawnNode.ICON_LOAD"
        label="More Icons..."
        style="push"
        menubarPath="object_actions"
        enablesFor="1"
        class="com.ember.workbench.gui.action.NodeAction">
      </action>
      <action
        id="com.ember.workbench.gui.view.IDrawnNode.ICON"
        label="Icon..."
        style="pulldown"
        menubarPath="object_actions"
        enablesFor="1"
        class="com.ember.workbench.gui.action.DrawnNodeIconPick">
      </action>
      <action
        id="com.ember.workbench.gui.action.NodeAction.LABEL"
        label="Label..."
        style="push"
        menubarPath="object_actions"
        enablesFor="1"
        class="com.ember.workbench.gui.action.NodeAction">
      </action>
    </objectContribution>
       
    <!-- Inode entries: popups for the node on the map AND node state view -->
    <objectContribution
          adaptable="true"
          objectClass="com.ember.workbench.capture.node.INode"
          id="com.ember.workbench.core.INode.Contribution">
      <action
        id="com.ember.workbench.gui.action.NodeAction.MULTINETWORK"
        label="Multinetwork node"
        style="toggle"
        state="false"
        menubarPath="object_actions"
        enablesFor="1"
        class="com.ember.workbench.gui.action.NodeAction">
      </action>
      <action
        id="com.ember.workbench.gui.action.NodeAction.EUI64"
        label="Assign EUI64..."
        style="push"
        menubarPath="object_actions"
        enablesFor="1"
        class="com.ember.workbench.gui.action.NodeAction">
      </action>
    </objectContribution>
    <objectContribution
          adaptable="false"
          id="com.ember.workbench.gui.CapableGroup"
          objectClass="com.ember.workbench.gui.CapableGroup">
       <action
             class="com.ember.workbench.gui.action.GroupAction"
             enablesFor="1"
             id="com.ember.workbench.gui.action.GroupAction.ADD"
             label="Add group"
             menubarPath="additions"
             state="true"
             style="push"/>
       <action
             class="com.ember.workbench.gui.action.GroupAction"
             enablesFor="1"
             id="com.ember.workbench.gui.action.GroupAction.DELETE"
             label="Delete group"
             menubarPath="additions"
             state="true"
             style="push"/>
       <action
             class="com.ember.workbench.gui.action.GroupAction"
             enablesFor="1"
             id="com.ember.workbench.gui.action.GroupAction.RENAME"
             label="Rename group"
             menubarPath="additions"
             state="true"
             style="push"/>
    </objectContribution>
    <objectContribution
          adaptable="false"
          id="com.ember.workbench.map.MapWidget"
          objectClass="com.ember.workbench.map.MapWidget">
