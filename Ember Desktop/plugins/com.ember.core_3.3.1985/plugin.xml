             class="com.ember.workbench.gui.action.MapBackgroundImage"
             id="com.ember.workbench.gui.action.MapBackgroundImage.CLEAR"
             label="Clear Background Image"
             menubarPath="additions"
             style="push">
       </action>
       <action
             class="com.ember.workbench.gui.action.MapBackgroundImage"
             id="com.ember.workbench.gui.action.MapBackgroundImage.LOAD"
             label="Load Background Image..."
             menubarPath="additions"
             style="push">
       </action>
       <action
             class="com.ember.workbench.gui.action.LoadLocationAction"
             id="com.ember.workbench.gui.action.LoadLocationAction.CLEAR"
             label="Clear locations"
             menubarPath="additions"
             style="push"/>
       <action
             class="com.ember.workbench.gui.action.LoadLocationAction"
             id="com.ember.workbench.gui.action.LoadLocationAction.LOAD"
             label="Load locations..."
             menubarPath="additions"
             style="push"/>
       <action
             class="com.ember.workbench.gui.action.LoadLocationAction"
             id="com.ember.workbench.gui.action.LoadLocationAction.SAVE"
             label="Save locations..."
             menubarPath="additions"
             style="push"/>
       <action
             class="com.ember.workbench.gui.action.ShowHiddenNodesAction"
             id="com.ember.workbench.gui.action.ShowHiddenNodesAction"
             label="Clear PAN filter"
             menubarPath="additions"
             style="push"/>
       <action
             class="com.ember.workbench.gui.action.PanFilterAction"
             id="com.ember.workbench.gui.action.PanFilterAction"
             label="Set PAN filter..."
             menubarPath="additions"
             style="push"/>
       <action
             id="com.ember.workbench.gui.action.MapEui64FormatAction"
             label="Display EUI64 with manufacturer"
             style="toggle"
             state="false"
             menubarPath="additions"
             enablesFor="1"
             class="com.ember.workbench.gui.action.MapEui64FormatAction">
      </action>
       <action
             class="com.ember.workbench.gui.action.OrganizeMapAction"
             id="com.ember.workbench.gui.action.OrganizeMapAction"
             label="Organize Map"
             menubarPath="additions"
             style="pulldown"/>
